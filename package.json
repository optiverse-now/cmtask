{
  "name": "cmtask",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "APP_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "build:staging": "APP_ENV=staging next build",
    "build:production": "APP_ENV=production next build",
    "start:staging": "APP_ENV=staging next start",
    "start:production": "APP_ENV=production next start",
    "lint": "next lint",
    "test": "jest --ci",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "prisma:dev": "dotenv -e .env.development -- prisma",
    "prisma:staging": "dotenv -e .env.staging -- prisma",
    "prisma:prod": "dotenv -e .env.production -- prisma",
    "db:migrate:dev": "npm run prisma:dev migrate dev",
    "db:migrate:staging": "npm run prisma:staging migrate deploy",
    "db:migrate:prod": "npm run prisma:prod migrate deploy",
    "db:studio:dev": "npm run prisma:dev studio",
    "db:studio:staging": "npm run prisma:staging studio",
    "db:studio:prod": "npm run prisma:prod studio",
    "api": "tsx watch src/api/index.ts",
    "api:build": "tsx src/api/index.ts",
    "dev:api": "dotenv -e .env.development -- tsx watch src/api/index.ts",
    "staging:api": "dotenv -e .env.staging -- tsx src/api/index.ts",
    "prod:api": "dotenv -e .env.production -- tsx src/api/index.ts"
  },
  "dependencies": {
