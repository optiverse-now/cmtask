{
  "version": 2,
  "buildCommand": "npm run type-check && npm run lint && npm run test && npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "APP_ENV": {
      "development": "development",
      "preview": "staging",
      "production": "production"
    }
  }
}